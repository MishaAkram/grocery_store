<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aac.project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Items"/>
        <activity android:name=".Fruits"/>
        <activity android:name=".Vegetable"/>
        <activity android:name=".Meats"/>
        <activity android:name=".BakeryItems"/>
        <activity android:name=".Apple"/>
        <activity android:name=".Banana"/>
        <activity android:name=".ShowCart"/>
        <activity android:name=".Potato"/>
        <activity android:name=".Capsicum"/>
        <activity android:name=".TotalAmount"/>
        <activity android:name=".CurrentLocation"/>
    </application>

</manifest>